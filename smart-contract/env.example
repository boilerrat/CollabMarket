# Copy to ".env" for deployments. Do NOT commit .env.

# Deployer EOA private key (0x-prefixed). Use a fresh deployer wallet.
PRIVATE_KEY=

# RPC URLs for Base networks
BASE_RPC_URL=
BASE_SEPOLIA_RPC_URL=

# USDC address on the target network.
# Required on Base/Base Sepolia.
# On localhost, the deploy script will deploy MockUSDC if left empty.
USDC_ADDRESS=


